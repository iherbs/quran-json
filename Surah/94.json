[{"id":6091,"surah":94,"no_ayah":1,"theme":"","text_ayah":"\u0627\u064e\u0644\u064e\u0645\u0652 \u0646\u064e\u0634\u0652\u0631\u064e\u062d\u0652 \u0644\u064e\u0643\u064e \u0635\u064e\u062f\u0652\u0631\u064e\u0643\u064e\u06d9","text_id":"Bukankah Kami telah melapangkan dadamu (Nabi Muhammad),","text_en":"HAVE WE NOT opened up your breast","transliteration":"a lam nasyraḥ laka ṣadrak","no_fn":"","text_fn":"","juz":30,"hizb":60,"rub_al_hizb":239,"page":596},{"id":6092,"surah":94,"no_ayah":2,"theme":"","text_ayah":"\u0648\u064e\u0648\u064e\u0636\u064e\u0639\u0652\u0646\u064e\u0627 \u0639\u064e\u0646\u0652\u0643\u064e \u0648\u0650\u0632\u0652\u0631\u064e\u0643\u064e\u06d9","text_id":"meringankan beban (tugas-tugas kenabian) darimu","text_en":"And removed your burden","transliteration":"wa waḍa'nā 'angka wizrak","no_fn":"","text_fn":"","juz":30,"hizb":60,"rub_al_hizb":239,"page":596},{"id":6093,"surah":94,"no_ayah":3,"theme":"","text_ayah":"\u0627\u0644\u0651\u064e\u0630\u0650\u064a\u0652\u0653 \u0627\u064e\u0646\u0652\u0642\u064e\u0636\u064e \u0638\u064e\u0647\u0652\u0631\u064e\u0643\u064e\u06d9","text_id":"yang memberatkan punggungmu,","text_en":"Which had left you devoid of hope,","transliteration":"allażī angqaḍa ẓahrak","no_fn":"","text_fn":"","juz":30,"hizb":60,"rub_al_hizb":239,"page":596},{"id":6094,"surah":94,"no_ayah":4,"theme":"","text_ayah":"\u0648\u064e\u0631\u064e\u0641\u064e\u0639\u0652\u0646\u064e\u0627 \u0644\u064e\u0643\u064e \u0630\u0650\u0643\u0652\u0631\u064e\u0643\u064e\u06d7 ","text_id":"dan meninggikan (derajat)-mu (dengan selalu) menyebut-nyebut (nama)-mu?","text_en":"And exalted your fame?","transliteration":"wa rafa'nā laka żikrak","no_fn":"","text_fn":"","juz":30,"hizb":60,"rub_al_hizb":239,"page":596},{"id":6095,"surah":94,"no_ayah":5,"theme":"","text_ayah":"\u0641\u064e\u0627\u0650\u0646\u0651\u064e \u0645\u064e\u0639\u064e \u0627\u0644\u0652\u0639\u064f\u0633\u0652\u0631\u0650 \u064a\u064f\u0633\u0652\u0631\u064b\u0627\u06d9","text_id":"Maka, sesungguhnya beserta kesulitan ada kemudahan.","text_en":"Surely with hardship there is ease.","transliteration":"fa inna ma'al-'usri yusrā","no_fn":"","text_fn":"","juz":30,"hizb":60,"rub_al_hizb":239,"page":596},{"id":6096,"surah":94,"no_ayah":6,"theme":"","text_ayah":"\u0627\u0650\u0646\u0651\u064e \u0645\u064e\u0639\u064e \u0627\u0644\u0652\u0639\u064f\u0633\u0652\u0631\u0650 \u064a\u064f\u0633\u0652\u0631\u064b\u0627\u06d7 ","text_id":"Sesungguhnya beserta kesulitan ada kemudahan.","text_en":"With hardship indeed there is ease.","transliteration":"inna ma'al-'usri yusrā","no_fn":"","text_fn":"","juz":30,"hizb":60,"rub_al_hizb":239,"page":596},{"id":6097,"surah":94,"no_ayah":7,"theme":"","text_ayah":"\u0641\u064e\u0627\u0650\u0630\u064e\u0627 \u0641\u064e\u0631\u064e\u063a\u0652\u062a\u064e \u0641\u064e\u0627\u0646\u0652\u0635\u064e\u0628\u0652\u06d9","text_id":"Apabila engkau telah selesai (dengan suatu kebajikan), teruslah bekerja keras (untuk kebajikan yang lain)","text_en":"So when you are free work diligently,","transliteration":"fa iżā faragta fanṣab","no_fn":"","text_fn":"","juz":30,"hizb":60,"rub_al_hizb":239,"page":596},{"id":6098,"surah":94,"no_ayah":8,"theme":"","text_ayah":"\u0648\u064e\u0627\u0650\u0644\u0670\u0649 \u0631\u064e\u0628\u0651\u0650\u0643\u064e \u0641\u064e\u0627\u0631\u0652\u063a\u064e\u0628\u0652\u08d6","text_id":"dan hanya kepada Tuhanmu berharaplah!","text_en":"And turn to your Lord with all your love.","transliteration":"wa ilā rabbika fargab","no_fn":"","text_fn":"","juz":30,"hizb":60,"rub_al_hizb":239,"page":596}]