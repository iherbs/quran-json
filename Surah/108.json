[{"id":6205,"surah":108,"no_ayah":1,"theme":"","text_ayah":"\u0627\u0650\u0646\u0651\u064e\u0627\u0653 \u0627\u064e\u0639\u0652\u0637\u064e\u064a\u0652\u0646\u0670\u0643\u064e \u0627\u0644\u0652\u0643\u064e\u0648\u0652\u062b\u064e\u0631\u064e\u06d7 ","text_id":"Sesungguhnya Kami telah memberimu (Nabi Muhammad) nikmat yang banyak.","text_en":"HAVE SURELY given you pre-eminence (in numbers and following);","transliteration":"innā a'ṭainākal-kauṡar","no_fn":"","text_fn":"","juz":30,"hizb":60,"rub_al_hizb":240,"page":602},{"id":6206,"surah":108,"no_ayah":2,"theme":"","text_ayah":"\u0641\u064e\u0635\u064e\u0644\u0651\u0650 \u0644\u0650\u0631\u064e\u0628\u0651\u0650\u0643\u064e \u0648\u064e\u0627\u0646\u0652\u062d\u064e\u0631\u0652\u06d7 ","text_id":"Maka, laksanakanlah salat karena Tuhanmu dan berkurbanlah!","text_en":"So serve your Lord with full dedication and sacrifice.","transliteration":"fa ṣalli lirabbika wan-ḥar","no_fn":"","text_fn":"","juz":30,"hizb":60,"rub_al_hizb":240,"page":602},{"id":6207,"surah":108,"no_ayah":3,"theme":"","text_ayah":"\u0627\u0650\u0646\u0651\u064e \u0634\u064e\u0627\u0646\u0650\u0626\u064e\u0643\u064e \u0647\u064f\u0648\u064e \u0627\u0644\u0652\u0627\u064e\u0628\u0652\u062a\u064e\u0631\u064f\u08d6","text_id":"Sesungguhnya orang yang membencimu, dialah yang terputus (dari rahmat Allah).","text_en":"It is surely your opponents whose line will come to end.","transliteration":"inna syāni`aka huwal-abtar","no_fn":"","text_fn":"","juz":30,"hizb":60,"rub_al_hizb":240,"page":602}]