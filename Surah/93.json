[{"id":6080,"surah":93,"no_ayah":1,"theme":"","text_ayah":"\u0648\u064e\u0627\u0644\u0636\u0651\u064f\u062d\u0670\u0649\u06d9","text_id":"Demi waktu duha","text_en":"I CALL TO witness the early hours of morning,","transliteration":"waḍ-ḍuḥā","no_fn":"","text_fn":"","juz":30,"hizb":60,"rub_al_hizb":238,"page":596},{"id":6081,"surah":93,"no_ayah":2,"theme":"","text_ayah":"\u0648\u064e\u0627\u0644\u0651\u064e\u064a\u0652\u0644\u0650 \u0627\u0650\u0630\u064e\u0627 \u0633\u064e\u062c\u0670\u0649\u06d9","text_id":"dan demi waktu malam apabila telah sunyi,","text_en":"And the night when dark and still,","transliteration":"wal-laili iżā sajā","no_fn":"","text_fn":"","juz":30,"hizb":60,"rub_al_hizb":238,"page":596},{"id":6082,"surah":93,"no_ayah":3,"theme":"","text_ayah":"\u0645\u064e\u0627 \u0648\u064e\u062f\u0651\u064e\u0639\u064e\u0643\u064e \u0631\u064e\u0628\u0651\u064f\u0643\u064e \u0648\u064e\u0645\u064e\u0627 \u0642\u064e\u0644\u0670\u0649\u06d7 ","text_id":"Tuhanmu (Nabi Muhammad) tidak meninggalkan dan tidak (pula) membencimu.","text_en":"Your Lord has neither left you, nor despises you.","transliteration":"mā wadda'aka rabbuka wa mā qalā","no_fn":"","text_fn":"","juz":30,"hizb":60,"rub_al_hizb":238,"page":596},{"id":6083,"surah":93,"no_ayah":4,"theme":"","text_ayah":"\u0648\u064e\u0644\u064e\u0644\u0652\u0627\u0670\u062e\u0650\u0631\u064e\u0629\u064f \u062e\u064e\u064a\u0652\u0631\u064c \u0644\u0651\u064e\u0643\u064e \u0645\u0650\u0646\u064e \u0627\u0644\u0652\u0627\u064f\u0648\u0652\u0644\u0670\u0649\u06d7 ","text_id":"Sungguh, akhirat itu lebih baik bagimu daripada yang permulaan (dunia).","text_en":"What is to come is better for you than what has gone before;","transliteration":"wa lal-ākhiratu khairul laka minal-ụlā","no_fn":"","text_fn":"","juz":30,"hizb":60,"rub_al_hizb":238,"page":596},{"id":6084,"surah":93,"no_ayah":5,"theme":"","text_ayah":"\u0648\u064e\u0644\u064e\u0633\u064e\u0648\u0652\u0641\u064e \u064a\u064f\u0639\u0652\u0637\u0650\u064a\u0652\u0643\u064e \u0631\u064e\u0628\u0651\u064f\u0643\u064e \u0641\u064e\u062a\u064e\u0631\u0652\u0636\u0670\u0649\u06d7 ","text_id":"Sungguh, kelak (di akhirat nanti) Tuhanmu pasti memberikan karunia-Nya kepadamu sehingga engkau rida.","text_en":"For your Lord will certainly give you, and you will be content.","transliteration":"wa lasaufa yu'ṭīka rabbuka fa tarḍā","no_fn":"","text_fn":"","juz":30,"hizb":60,"rub_al_hizb":238,"page":596},{"id":6085,"surah":93,"no_ayah":6,"theme":"","text_ayah":"\u0627\u064e\u0644\u064e\u0645\u0652 \u064a\u064e\u062c\u0650\u062f\u0652\u0643\u064e \u064a\u064e\u062a\u0650\u064a\u0652\u0645\u064b\u0627 \u0641\u064e\u0627\u0670\u0648\u0670\u0649\u06d6 ","text_id":"Bukankah Dia mendapatimu sebagai seorang yatim, lalu Dia melindungi(-mu);","text_en":"Did He not find you an orphan and take care of you?","transliteration":"a lam yajidka yatīman fa āwā","no_fn":"","text_fn":"","juz":30,"hizb":60,"rub_al_hizb":238,"page":596},{"id":6086,"surah":93,"no_ayah":7,"theme":"","text_ayah":"\u0648\u064e\u0648\u064e\u062c\u064e\u062f\u064e\u0643\u064e \u0636\u064e\u0627\u06e4\u0644\u0651\u064b\u0627 \u0641\u064e\u0647\u064e\u062f\u0670\u0649\u06d6 ","text_id":"mendapatimu sebagai seorang yang tidak tahu (tentang syariat), lalu Dia memberimu petunjuk (wahyu);","text_en":"Did He not find you perplexed, and show you the way?","transliteration":"wa wajadaka ḍāllan fa hadā","no_fn":"","text_fn":"","juz":30,"hizb":60,"rub_al_hizb":238,"page":596},{"id":6087,"surah":93,"no_ayah":8,"theme":"","text_ayah":"\u0648\u064e\u0648\u064e\u062c\u064e\u062f\u064e\u0643\u064e \u0639\u064e\u0627\u06e4\u0649\u0655\u0650\u0644\u064b\u0627 \u0641\u064e\u0627\u064e\u063a\u0652\u0646\u0670\u0649\u06d7 ","text_id":"dan mendapatimu sebagai seorang yang fakir, lalu Dia memberimu kecukupan?","text_en":"Did He not find you poor and enrich you?","transliteration":"wa wajadaka 'ā`ilan fa agnā","no_fn":"","text_fn":"","juz":30,"hizb":60,"rub_al_hizb":238,"page":596},{"id":6088,"surah":93,"no_ayah":9,"theme":"","text_ayah":"\u0641\u064e\u0627\u064e\u0645\u0651\u064e\u0627 \u0627\u0644\u0652\u064a\u064e\u062a\u0650\u064a\u0652\u0645\u064e \u0641\u064e\u0644\u064e\u0627 \u062a\u064e\u0642\u0652\u0647\u064e\u0631\u0652\u06d7 ","text_id":"Terhadap anak yatim, janganlah engkau berlaku sewenang-wenang.","text_en":"So do not oppress the orphan,","transliteration":"fa ammal-yatīma fa lā taq-har","no_fn":"","text_fn":"","juz":30,"hizb":60,"rub_al_hizb":238,"page":596},{"id":6089,"surah":93,"no_ayah":10,"theme":"","text_ayah":"\u0648\u064e\u0627\u064e\u0645\u0651\u064e\u0627 \u0627\u0644\u0633\u0651\u064e\u0627\u06e4\u0649\u0655\u0650\u0644\u064e \u0641\u064e\u0644\u064e\u0627 \u062a\u064e\u0646\u0652\u0647\u064e\u0631\u0652","text_id":"Terhadap orang yang meminta-minta, janganlah engkau menghardik.","text_en":"And do not drive the beggar away,","transliteration":"wa ammas-sā`ila fa lā tan-har","no_fn":"","text_fn":"","juz":30,"hizb":60,"rub_al_hizb":238,"page":596},{"id":6090,"surah":93,"no_ayah":11,"theme":"","text_ayah":"\u0648\u064e\u0627\u064e\u0645\u0651\u064e\u0627 \u0628\u0650\u0646\u0650\u0639\u0652\u0645\u064e\u0629\u0650 \u0631\u064e\u0628\u0651\u0650\u0643\u064e \u0641\u064e\u062d\u064e\u062f\u0651\u0650\u062b\u0652\u08d6","text_id":"Terhadap nikmat Tuhanmu, nyatakanlah (dengan bersyukur).","text_en":"And keep recounting the favours of your Lord.","transliteration":"wa ammā bini'mati rabbika fa ḥaddiṡ","no_fn":"","text_fn":"","juz":30,"hizb":60,"rub_al_hizb":238,"page":596}]